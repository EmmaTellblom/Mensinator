CREATE TABLE cycle(
id INTEGER PRIMARY KEY AUTOINCREMENT,
start_date TEXT NOT NULL,
end_date TEXT,
length INTEGER,
luteal_phase_length INTEGER
);


-- SELECT QUERIES --
--------------------

getAllCycles:
SELECT * FROM cycle;

getAllCyclesByDate:
SELECT * FROM cycle WHERE start_date BETWEEN ? AND ?;


